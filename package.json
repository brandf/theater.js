{
  "name": "theater.js",
  "version": "0.1.13",
  "description": "A 3d scene graph.",
  "main": "dist/theater.js",
  "scripts": {
    "copy": "cp ./dist/* ./integration/node_modules/theater.js/dist -v",
    "build": "webpack --progress --colors && npm run copy",
    "build_opt": "webpack --progress --colors --optimize-minimize && npm run copy",
    "publish": "npm run build_opt",
    "test": "mocha \"./src/**/*.spec.js\" --require babel-register --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandf/theater.js.git"
  },
  "author": "Brandon Furtwangler <brandon.furtwangler@gmail.com> (https://brandf.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandf/theater.js/issues"
  },
  "homepage": "https://github.com/brandf/theater.js#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "gl-matrix": "^2.3.2"
  }
}
